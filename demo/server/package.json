{
  "name": "@raincatcher/demo-server",
  "version": "1.0.0",
  "description": "Raincatcher demo application server using express",
  "types": "src/index.ts",
  "main": "src/index.ts",
  "author": "feedhenry-raincatcher@redhat.com",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "del src/*.js src/**/*.js src/*.map src/**/*.map test/**/*.js test/**/*.map",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "test": "nyc mocha"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "check-coverage": true,
    "lines": 75,
    "functions": 100,
    "branches": 80
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/cookie-parser": "^1.3.30",
    "@types/express": "^4.0.35",
    "@types/mocha": "^2.2.41",
    "@types/morgan": "^1.7.32",
    "@types/serve-favicon": "^2.2.28",
    "del-cli": "^1.0.0",
    "grunt": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-env": "^0.4.4",
    "grunt-exec": "^2.0.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.1",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.0.4",
    "typescript": "^2.3.4"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.3",
    "morgan": "^1.8.1",
    "serve-favicon": "^2.4.3"
  }
}
